<div class="overflow-auto">
<%@rooms.order(id: :desc).each do |room|%>
  <%= link_to room do%>
    <div class="card mb-2" data-spy="scroll">
      <div class="card-body">
        <b><%= room.name%></b><br>
        <small><%= room.users.uniq.count%>member</small>
      </div>
    </div>
  <%end%>  
<%end%>
</div>